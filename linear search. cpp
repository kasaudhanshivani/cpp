#include<bits/stdc++.h>
using namespace std;
int linearsearch(int n,int arr[], int target){
	int ans = -1;
	for(int i=0; i<n; i++){
		if(arr[i]==target){
			ans = i;
		}
	}
	return ans;

}
int main(){
	int n;
	int arr[n];
	for(int i=0;i<n;i++){
 		cin>>arr[i];
		cout<<arr[i]<<" ";
	}
	int target;
	cin>>target;
 	cout<<linearsearch(n, arr, target);
	return 0;
}
